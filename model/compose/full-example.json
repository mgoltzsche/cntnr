{
  "services": {
    "foo": {
      "image": "docker://redis",
      "build": {
        "context": "./dir",
        "dockerfile": "Dockerfile",
        "args": {
          "foo": "bar"
        }
      },
      "entrypoint": [
        "/code/entrypoint.sh",
        "-p",
        "3000"
      ],
      "command": [
        "bundle",
        "exec",
        "thin",
        "-p",
        "3000"
      ],
      "working_dir": "/code",
      "environment": {
        "BAR": "bar_from_env_file_2",
        "BAZ": "baz_from_service_def",
        "FOO": "foo_from_env_file",
        "QUX": ""
      },
      "user": {
        "uid": "someone",
        "gid": "someone"
      },
      "cap_add": [
        "ALL"
      ],
      "cap_drop": [
        "NET_ADMIN",
        "SYS_ADMIN"
      ],
      "seccomp": "default",
      "hostname": "my-web-container",
      "domainname": "foo.com",
      "dns": [
        "8.8.8.8",
        "9.9.9.9"
      ],
      "dns_search": [
        "dc1.example.com",
        "dc2.example.com"
      ],
      "extra_hosts": [
